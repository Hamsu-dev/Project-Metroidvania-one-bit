config_version=5

[application]

config/name="Project-Metroidvania-one-bit"
run/main_scene="res://world/world.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://player/player_ship.png"

[autoload]

Utils="*res://Singleton/Utils.gd"
Events="*res://Singleton/events.gd"
PlayerStats="*res://player/player_stats.tscn"
MainInstances="*res://Singleton/MainInstances.gd"
DifficultyManager="*res://Singleton/DifficultyManager.gd"
GameStats="*res://Singleton/GameStats.gd"
DamageOutput="*res://Singleton/damage_output.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/viewport_width=620
window/size/viewport_height=320
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://ui/cursor-target-5.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg")

[gui]

theme/custom="res://ui/default_theme.tres"
theme/custom_font="res://ui/pixel_font.ttf"

